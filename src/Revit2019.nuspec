<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
    <metadata>
        <!-- The identifier that must be unique within the hosting gallery -->
        <id>Autodesk.Revit.SDK</id>

        <!-- The package version number that is used when resolving dependencies -->
        <version>2019.2.0</version>

        <!-- The icon is used in Visual Studio's package manager UI -->
        <iconUrl>https://raw.githubusercontent.com/CodeCavePro/RevitSDK/master/assets/Revit-2017.png</iconUrl>

        <!-- Authors contain text that appears directly on the gallery -->
        <authors>Zhmayev Yaroslav</authors>

        <!-- 
            Owners are typically nuget.org identities that allow gallery
            users to easily find other packages by the same owners.  
        -->
        <owners>salaros, CodeCave</owners>
        
    	<requireLicenseAcceptance>true</requireLicenseAcceptance>
        
         <!-- Project URL provides a link for the gallery -->
        <projectUrl>https://www.autodesk.com/developer-network/platform-technologies/revit</projectUrl>

         <!-- License information is displayed on the gallery -->
        <license type="file">LICENSE.txt</license>

        <!-- 
            The description can be used in package manager UI. Note that the
            nuget.org gallery uses information you add in the portal. 
        -->
        <description>The Software Development Toolkit (SDK) provides extensive .NET code samples and documentation to help you get started developing with the Revit API.</description>

        <!-- Copyright information -->
        <copyright>Â© Autodesk Inc. All rights reserved</copyright>

        <!-- Tags appear in the gallery and can be used for tag searches -->
        <tags>revit sdk api autodesk revitapi revitsdk 2014 2015 2016 2017 2018 2019 2020</tags>
    </metadata>

    <files>
	<file src="..\assets\LICENSE.txt" target="" />
        <file src="..\lib\2019\*" target="lib\net47" />
    </files>
</package>